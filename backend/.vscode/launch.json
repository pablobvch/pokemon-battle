{
    "version": "0.2.0",
    "configurations": [
       {
         "type": "node",
         "request": "launch",
         "name": "Launch NestJS App",
         "runtimeExecutable": "npm",
         "runtimeArgs": [
           "run-script",
           "start:dev"
         ],
         "console": "integratedTerminal",
         "internalConsoleOptions": "neverOpen",
         "skipFiles": [
           "<node_internals>/**"
         ]
       }
    ]
   }
